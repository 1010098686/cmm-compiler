struct Vector{
    float x;
    float y;
    float list[5];
};

struct Vector f(struct Vector A, struct Vector B) {
    struct Vector r;
    int i, N = 5;
    r.x = A.x + B.x;
    r.y = A.y + B.y;
    while(i < N) {
        r.list[i] = A.list[i] + B.list[i];
    }
    return r.list;
}